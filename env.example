# 飞书开放平台配置 / Feishu Open Platform Configuration
FEISHU_APP_ID=your_feishu_app_id
FEISHU_APP_SECRET=your_feishu_app_secret
FEISHU_VERIFICATION_TOKEN=your_verification_token

# Google Gemini AI配置 / Google Gemini AI Configuration  
GEMINI_API_KEY=your_gemini_api_key

# Figma API配置 / Figma API Configuration
FIGMA_ACCESS_TOKEN=your_figma_access_token

# 测试配置 / Test Configuration
TEST_MODE=development
LOG_LEVEL=INFO

# 报告配置 / Report Configuration  
REPORT_OUTPUT_DIR=./reports
SCREENSHOT_DIR=./screenshots

# 浏览器配置 / Browser Configuration (可选 / Optional)
