# BROWSER_HEADLESS=true  # true 或 false
# BROWSER_LANGUAGE=en-US  # 浏览器语言设置

# 工作流配置 / Workflow Configuration (可选 / Optional)
# DEFAULT_DEVICE=desktop  # desktop, mobile, tablet
# DEFAULT_WAIT_TIME=3  # 页面加载等待时间(秒)
# MAX_COMPARISON_SIZE=2048  # 比较图像最大尺寸(像素) 